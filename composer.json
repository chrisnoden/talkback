    "version": "0.1.0-dev",
    "homepage": "https://github.com/chrisnoden/talkback",
    "type": "library",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Chris Noden",
            "email": "chris.noden@gmail.com"
        }
