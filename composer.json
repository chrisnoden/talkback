{
    "name": "chrisnoden/talkback",
    "description": "Logging and messaging library that builds upon Psr-3 with Growl, Prowl, email and other modules",
    "keywords": ["psr-3", "growl", "prowl", "logging"],
    "license": "Apache-2.0",
    "version": "0.1.0-dev",
    "homepage": "https://github.com/chrisnoden/talkback",
    "type": "library",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Chris Noden",
            "email": "chris.noden@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "Talkback": "src/" },
        "classmap": [ "src/Net_Growl", "src/Net_Growl/Growl" ]
    },
    "require": {
        "php": ">=5.3.21",
        "psr/log": "1.*"
    },
    "require-dev": {
        "xenji/prowlphp": "1.*"
    },
    "suggest": {
        "xenji/prowlphp": "1.0.*"
    }
}